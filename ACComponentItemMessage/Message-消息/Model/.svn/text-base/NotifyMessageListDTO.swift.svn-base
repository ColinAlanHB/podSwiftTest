import Foundation
import ACComponentItemLib

/// 通知消息列表
class UserNotifyMessageListDTO : DatalistBaseModel {
    
    var id : String?           // 主键
    var title : String?        // 标题
    var summary : String?      // 简述
    var openType : Int?        // 打开类型(无链接、内链、外链)
    var openParams : String?    // 打开类型对应的资源标识
    var showDetail : Bool?     // 是否显示详情
    var pushType : Int?        // 推送类型(文字或链接)
    var coverPicUrl : String?  // 封面图片url
    var createTimeStr : String?        //展示时间
    
    required init() {
        
    }
}

/// 通知消息详情
class UserNotifyMessageDTO : DatalistBaseModel {
    
    var id : String?
    /**
     * 标题
     */
    var title : String?
    /**
     * 通知内容
     */
    var content : String?
    /**
     * 来源类型
     */
    var sourceType : Int?
    /**
     * 来源资源类型
     */
    var sourceResourceId : String?
    /**
     * 打开方式
     */
    var openType : Int?
    /**
     * 打开方式需要的参数
     */
    var openParams : String?
    /**
     * 创建时间
     */
    var createTime : Int64?
    
    /**
     * 时间字符串
     */
    var createTimeStr : String?
    
    /**
     * 用户标识
     */
    var userId : String?
    
    /**
     * 是否已读
     */
    var readed : Int?
    
    // 作者
    var author: String?
    
    required init() {
        
    }
}
